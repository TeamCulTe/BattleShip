<UserControl x:Class="BattleShip.UserControls.CustomMapButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BattleShip.UserControls"
             mc:Ignorable="d">
    <Button x:Name="mapButton">
        <Image x:Name="buttonImage" Stretch="Fill" Source="{Binding Image, Mode=TwoWay}"></Image>
    </Button>
</UserControl>
